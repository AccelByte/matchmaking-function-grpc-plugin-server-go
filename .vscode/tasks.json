{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "proto",
        "type": "shell",
        "command": "make proto",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "problemMatcher": []
      },
      {
        "label": "build",
        "type": "shell",
        "command": "make build",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "dependsOn": ["proto"],
        "problemMatcher": []
      },
      {
        "label": "run",
        "type": "shell",
        "command": "go run ./main.go",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }
  